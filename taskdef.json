{
    "executionRoleArn": "arn:aws:iam::387279199735:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "ecs-cicd-demo",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "entryPoint": ["./docker_script.sh"],
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "ecs-cicd-demo-web"
}