{
    "executionRoleArn": "arn:aws:iam::387279199735:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "webapp",
            "image": "387279199735.dkr.ecr.us-west-2.amazonaws.com/ecs_cicd_demo",
            "essential": true,
            "entryPoint": ["./docker_script.sh"],
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                },
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "codepipeline-cicd-demo"
}